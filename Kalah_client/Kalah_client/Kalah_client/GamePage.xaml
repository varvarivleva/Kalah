<Page x:Class="KalahClient.GamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Game">
    <Grid Height="70" Width="164">
        <!-- UI игры -->
        <TextBlock Text="Игра в Калах" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="9" Height="17" Width="54" />
        <!-- Добавить кнопки/лунки для игры -->
        <Button Content="Завершить игру" Width="58" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left"
                Click="EndGameButton_Click" Margin="55,48,0,0" FontSize="7" />
    </Grid>
</Page>
